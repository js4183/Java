package while문;

import java.util.Scanner;

public class Ex01예제4 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		System.out.print("현재 몸무게 : ");
		int n = sc.nextInt();
		
		System.out.print("목표 몸무게 : ");
		int g = sc.nextInt();
		
		int w = 0;
		
		while(n<=g) {
			System.out.printf("%d주차 감량 몸무게");
		}

	}

}
